
@{
    ViewBag.Title = "RegisterSuperUser";
    Layout = null;
}

<link href="~/Scripts/css/layui.css" rel="stylesheet" />
@{
    if (ViewBag.UserList != null)
    {
        var dataList = ViewBag.UserList as List<St.Domain.Entity.SuperUser.SUser>;
        if (dataList.Count == 0)
        {
            return;
        }

        <table class="layui-table">
            <colgroup>
                <col width="150">
                <col width="200">
                <col>
            </colgroup>
            <thead>
                <tr>
                    <th>登录名</th>
                    <th>昵称</th>
                    <th>密码</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in dataList)
                {
                    <tr>
                        <td>@item.Name</td>
                        <td>@item.NickName</td>
                        <td>@item.PassWord</td>
                    </tr>
                }
            </tbody>
        </table>
    }
}

